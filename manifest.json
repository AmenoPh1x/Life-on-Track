{
    "manifest_version": 2,
    "name": "Life on track",
    "version": "0.3",

    "description": "Domain spend time tracker, blocker and redirecter.",
    "homepage_url": "https://github.com/christianamenos/Life-on-Track",

    "icons": {
        "48": "icons/border-48.png"
    },

    "permissions": [
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "*://*/*"
    ],

    "background": {
        "scripts": [
            "back/js/defaultBannedUrls.js",
            "back/js/background.js"
        ]
    },

    "web_accessible_resources": [
        "front/build/index.html"
    ]
}